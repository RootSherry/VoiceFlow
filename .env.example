# 基础
NODE_ENV=development
PORT=3000

# 队列（BullMQ）
REDIS_URL=redis://127.0.0.1:6379

# 持久化（本机）
DATA_DIR=./data
SQLITE_PATH=./data/voiceflow.sqlite3

# 上传限制（字节，默认 50MB）
UPLOAD_MAX_BYTES=52428800

# AI（仅服务端/worker 使用）
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash-preview-09-2025
GEMINI_MAX_RETRIES=5

# Worker
WORKER_CONCURRENCY=1
JOB_ATTEMPTS=3

